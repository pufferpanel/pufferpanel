{
  "keepDefaultCss": true,
  "sidebarClosedBelow": 1200,
  "settings": {
    "mode": {
      "type": "class",
      "tkey": "common.theme.Mode",
      "options": [
        {
          "value": "mode-auto",
          "tkey": "common.theme.mode.Auto",
          "default": true
        },
        {
          "value": "mode-light",
          "tkey": "common.theme.mode.Light"
        },
        {
          "value": "mode-dark",
          "tkey": "common.theme.mode.Dark"
        }
      ]
    },
    "color": {
      "type": "color",
      "var": "--primary",
      "default": "#07a7e3",
      "derive": [
        {
          "type": "contrast",
          "foreground": true,
          "options": ["#eee", "#333"],
          "var": "--text-primary"
        },
        {
          "type": "opacity",
          "opacity": 0.15,
          "var": "--primary-hover"
        },
        {
          "type": "hueShift",
          "hueShift": 180,
          "var": "--primary-complement"
        }
      ],
      "tkey": "common.theme.BaseColor"
    },
    "serverTypeDisplay": {
      "type": "class",
      "tkey": "common.theme.ServerTypeDisplay",
      "options": [
        {
          "value": "server-type-icon",
          "tkey": "common.theme.serverTypeDisplay.Icon",
          "default": true
        },
        {
          "value": "server-type-none",
          "tkey": "common.theme.serverTypeDisplay.None"
        }
      ]
    },
    "listType": {
      "type": "class",
      "tkey": "common.theme.ListType",
      "options": [
        {
          "value": "list-type-grid",
          "tkey": "common.theme.listType.Grid",
          "default": true
        },
        {
          "value": "list-type-list",
          "tkey": "common.theme.listType.List"
        }
      ]
    }
  }
}
